package com.sush.myiocprj;

public class Employee {
	
	private int id;
	private String name;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	public void show( ) {
		System.out.println("Hello " + this.id + " " + this.name);
	}

}
